{
  "name": "react-boiler",
  "version": "0.0.1",
  "descriptiion": "ReactJS boilerplate",
  "author": "Viktor Kotusenko",
  "scripts": {
    "babel:compile": "babel src --presets react,es2015 --out-dir static",
    "babel:watch": "babel src --presets env --out-dir static --watch",
    "lint": "standard",
    "start": "webpack-dev-server --config webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/roxolan/react-boiler.git"
  },
  "keywords": [
    "babel",
    "react",
    "webpack"
  ],
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "file-loader": "^0.11.1",
    "standard": "^10.0.2",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "express": "^4.15.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "webpack": "^2.5.1"
  }
}
